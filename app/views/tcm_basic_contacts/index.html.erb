<p id="notice"><%= notice %></p>

<h1>Listing Tcm Basic Contacts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tcm_basic_contacts.each do |tcm_basic_contact| %>
      <tr>
        <td><%= link_to 'Show', tcm_basic_contact %></td>
        <td><%= link_to 'Edit', edit_tcm_basic_contact_path(tcm_basic_contact) %></td>
        <td><%= link_to 'Destroy', tcm_basic_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tcm basic contact', new_tcm_basic_contact_path %>
