<p id="notice"><%= notice %></p>

<h1>Listing Tcm Patient Infos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tcm_patient_infos.each do |tcm_patient_info| %>
      <tr>
        <td><%= link_to 'Show', tcm_patient_info %></td>
        <td><%= link_to 'Edit', edit_tcm_patient_info_path(tcm_patient_info) %></td>
        <td><%= link_to 'Destroy', tcm_patient_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tcm patient info', new_tcm_patient_info_path %>
